{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Document": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": false
    },
    "link": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "person": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::person.person"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "articles"
    }
  }
}
