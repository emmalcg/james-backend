{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Work",
        "About",
        "Year"
      ],
      "required": true
    },
    "texts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::text.text",
      "inversedBy": "categories"
    },
    "article": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "categories"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "artworks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artwork.artwork",
      "mappedBy": "category"
    },
    "series": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::serie.serie",
      "inversedBy": "categories"
    }
  }
}
